[color]
	merge = auto
	status = auto
	branch = auto
	diff = auto
	ui = true
	blame = auto
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[user]
	name = icebreaker
	email = theicebreaker007@gmail.com
[push]
	default = current
[alias]
	hub = !hub
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
