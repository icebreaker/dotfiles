alias g='git'
alias gs='g status'
alias ga='g add'
alias gc='g commit -m'
alias gp='g push origin'
alias gpm='g push origin master'
alias gph='g push heroku master'
alias gu='g pull origin'
alias gum='g pull origin master'
alias gl='g log'
alias gug='g pull origin gh-pages'
alias ggp='g symbolic-ref HEAD refs/heads/gh-pages && rm .git/index && g clean -fdx'
alias gd='g diff'
alias gdc='g diff --cached'
alias ghb='git hub browse'
alias gnb='git checkout -b'
alias gcb='git checkout'
